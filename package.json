{
  "name": "arc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "start:mock": "npm run mockDev",
    "server": "babel-node server.js",
    "env:dev": "cross-env NODE_ENV=development",
    "env:mockDev": "cross-env NODE_ENV=mockDev",
    "env:prod": "cross-env NODE_ENV=production",
    "env:example": "cross-env SOURCE=src-example",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src src-example test .storybook webpack.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:example": "npm run env:example -- npm run storybook",
    "dev": "npm run env:dev -- webpack-dev-server --progress",
    "mockDev": "npm run env:mockDev -- webpack-dev-server --progress",
    "dev:example": "npm run env:example -- npm run dev",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack --progress",
    "build:example": "npm run env:example -- npm run build"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "src-example",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/test/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/test/actionsMock.js"
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "delay": "^2.0.0",
    "dora": "^0.4.3",
    "enzyme": "^2.7.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.6",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.0",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.26.0",
    "jest-cli": "^19.0.0",
    "json-server": "^0.10.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "node-sass": "^4.5.3",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.1.2",
    "webpack-dev-server": "^2.3.0",
    "webpack-md5-hash": "^0.0.5",
    "yargs-parser": "^7.0.0"
  },
  "dependencies": {
    "antd": "^2.10.4",
    "babel-polyfill": "^6.20.0",
    "faker": "^4.1.0",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "normalizr": "^3.0.1",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.2",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.5.2",
    "react-dates": "^12.1.1",
    "react-dom": "^15.5.4",
    "react-flexbox-grid": "^1.1.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-modal": "^1.6.5",
    "react-redux": "^5.0.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "recompose": "^0.23.5",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-saga": "^0.14.1",
    "reselect": "^3.0.1",
    "styled-components": "^1.4.4",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2",
    "validator": "^7.0.0",
    "whatwg-fetch": "^2.0.2"
  }
}
